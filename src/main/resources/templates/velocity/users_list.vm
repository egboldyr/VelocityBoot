<html>
    <body>
        <table>
            <tr>
                <th>Id</th>
                <th>Login</th>
                <th>Password</th>
                <th>Email</th>
            </tr>

            #foreach($user in $model["users"])
                <tr>
                    <td>$user.userId</td>
                    <td>$user.login</td>
                    <td>$user.password</td>
                    <td>$user.email</td>
                </tr>
            #end
        </table>
    </body>
</html>